{
  "name": "@ohbug-server/dashboard",
  "version": "1.0.0-alpha.0",
  "description": "ohbug dashboard server",
  "author": "chenyueban",
  "scripts": {
    "tsc": "tsc",
    "prebuild": "rimraf dist",
    "start": "cross-env NODE_ENV=development nest start --watch --debug --preserveWatchOutput\t",
    "build": "nest build",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@ohbug-server/common": "^1.0.0-alpha.0",
    "@ohbug-server/types": "^1.0.0-alpha.0",
    "cookie-parser": "^1.4.5",
    "nanoid": "^3.1.23",
    "nest-winston": "^1.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "source-map-trace": "^0.1.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/passport-jwt": "^3.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
